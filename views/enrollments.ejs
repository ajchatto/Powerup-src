        <td><%=data[i].offering_name%> </td>
        <td><%=data[i].start_date%> </td>
        <td><%=data[i].start_offering_time%> </td>
        <td><%=data[i].end_offering_time%> </td>
        <td><%=data[i].enrolled_date%> </td>

      </tr>
<%
}
%>

</table>



