</thead>

<%
  for(var i = 0;i < data.length;i++) {
%>

      <tr>
        <td nowrap><%=data[i].student_contact_name%> </td>
        <td nowrap><%=data[i].parent_contact_name%> </td>
        <td><%=data[i].parent_email_address%>, <BR> <%= data[i].parent_cell_phone%> </td>
